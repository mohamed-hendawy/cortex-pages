{
    "name": "cortex/pages",
    "type": "cortex-module",
    "description": "Cortex Pages is a frontend layer for an integral part of your Laravel content management system (CMS), it affords an easy, yet powerful way to create and manage pages with full control over their URLs, active status, titles, content, and other attributes.",
    "keywords": [
        "ajax",
        "async",
        "laravel",
        "rinvex",
        "pages",
        "cache",
        "page",
        "cms"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-pages/issues",
        "source": "https://github.com/rinvex/cortex-pages",
        "docs": "https://github.com/rinvex/cortex-pages/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-pages/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "Illuminate/auth": "~5.7.0",
        "Illuminate/console": "~5.7.0",
        "Illuminate/http": "~5.7.0",
        "Illuminate/routing": "~5.7.0",
        "Illuminate/support": "~5.7.0",
        "cortex/foundation": "dev-develop",
        "davejamesmiller/laravel-breadcrumbs": "^5.1.0",
        "laravelcollective/html": "~5.7.0",
        "league/fractal": "^0.17.0",
        "proengsoft/laravel-jsvalidation": "^2.2.0",
        "rinvex/composer": "dev-develop",
        "rinvex/menus": "dev-develop",
        "rinvex/pages": "dev-develop",
        "rinvex/support": "dev-develop",
        "rinvex/tenants": "dev-develop",
        "silber/bouncer": "v1.0.0-rc.1",
        "spatie/laravel-activitylog": "^2.5.0",
        "spatie/laravel-medialibrary": "^7.3.0",
        "yajra/laravel-datatables-buttons": "^3.2.0",
        "yajra/laravel-datatables-fractal": "^1.2.0",
        "yajra/laravel-datatables-html": "^3.7.0",
        "yajra/laravel-datatables-oracle": "^8.6.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.19.0",
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Cortex\\Pages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortex\\Pages\\Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cortex\\Pages\\Providers\\PagesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
