{
    "name": "cortex/pages",
    "description": "Cortex Pages is a frontend layer for an integral part of your Laravel content management system (CMS), it affords an easy, yet powerful way to create and manage pages with full control over their URLs, active status, titles, content, and other attributes.",
    "type": "cortex-module",
    "keywords": [
        "ajax",
        "async",
        "laravel",
        "rinvex",
        "pages",
        "cache",
        "page",
        "cms"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-pages/issues",
        "source": "https://github.com/rinvex/cortex-pages",
        "docs": "https://github.com/rinvex/cortex-pages/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-pages/contributors"
        }
    ],
    "require": {
        "php": "^7.4.0",
        "illuminate/auth": "^7.0.0",
        "illuminate/console": "^7.0.0",
        "illuminate/http": "^7.0.0",
        "illuminate/routing": "^7.0.0",
        "illuminate/support": "^7.0.0",
        "cortex/foundation": "^4.0.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.3.0",
        "laravel/helpers": "^1.2.0",
        "laravelcollective/html": "^7.0.0",
        "league/fractal": "^0.19.0",
        "proengsoft/laravel-jsvalidation": "^2.5.0",
        "rinvex/laravel-composer": "^4.0.0",
        "rinvex/laravel-menus": "^4.0.0",
        "rinvex/laravel-pages": "^4.0.0",
        "rinvex/laravel-support": "^4.0.0",
        "rinvex/laravel-tenants": "^4.0.0",
        "silber/bouncer": "v1.0.0-rc.7",
        "spatie/laravel-activitylog": "^3.13.0",
        "spatie/laravel-medialibrary": "^8.0.0",
        "yajra/laravel-datatables-buttons": "^4.9.0",
        "yajra/laravel-datatables-fractal": "^1.5.0",
        "yajra/laravel-datatables-html": "^4.23.0",
        "yajra/laravel-datatables-oracle": "^9.9.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.27.0",
        "phpunit/phpunit": "^9.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Cortex\\Pages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortex\\Pages\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cortex\\Pages\\Providers\\PagesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
